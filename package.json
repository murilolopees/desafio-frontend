    "@babel/runtime": "^7.11.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.12",
    "browser-sync-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "http-server": "^0.11.1",
    "laravel-mix": "^5.0.5",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
